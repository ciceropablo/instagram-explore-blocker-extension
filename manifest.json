{
  "manifest_version": 3,
  "name": "Instagram Explore Blocker",
  "description": "Extension to hide/block explore funcionality of Instagram",
  "version": "0.0.1",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/explore/hide.js"],
      "matches": ["https://*.instagram.com/*"]
    }
  ]
}
